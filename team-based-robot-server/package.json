{
  "name": "team-based-robot-server",
  "version": "1.0.0",
  "description": "Web application for team-based robot plugin",
  "author": "narospol pathong <naros.pol@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "git push heroku develop:master",
    "heroku-postbuild": "npm run build",
    "clean-db": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "clean-db-run": "knex migrate:rollback && knex migrate:latest && knex seed:run && backpack dev"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.6.0",
    "@nuxtjs/axios": "5.3.1",
    "base-64": "0.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.2",
    "bookshelf": "0.12.1",
    "bookshelf-cascade-delete": "2.0.1",
    "bookshelf-modelbase": "2.10.4",
    "compression": "^1.7.1",
    "cookie": "0.3.1",
    "cookie-parser": "1.4.3",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.9",
    "dotenv": "4.0.0",
    "express": "^4.15.3",
    "express-validator": "4.3.0",
    "file-loader": "1.1.6",
    "js-cookie": "2.2.0",
    "jsonwebtoken": "8.1.0",
    "knex": "0.19.5",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "nodemailer": "4.6.8",
    "nuxt": "^1.0.0",
    "pg": "7.4.1",
    "postgres-response-codes": "1.0.1",
    "pretty-error": "2.1.1",
    "ramda": "0.25.0",
    "socket.io": "^2.2.0",
    "source-map-support": "^0.4.15",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.6.2",
    "vuetify": "^1.1.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "backpack-core": "^0.4.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "happypack": "4.0.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.6"
  },
  "engines": {
    "node": "^8.1.4",
    "npm": "5.6.0"
  }
}
